{
  "entryPoints": ["src/index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": ["**/*test.ts", "**/node_modules/**"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "readme": "README.md",
  "name": "PnWKit 3.0",
  "includeVersion": true,
  "plugin": ["typedoc-plugin-markdown"],
  "githubPages": false,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  }
}