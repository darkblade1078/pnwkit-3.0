{
  "entryPoints": [
    "src/index.ts",
    "src/api/queries/*.ts",
    "src/utilities/**/*.ts"
  ],
  "out": "docs",
  "exclude": [
    "**/*.test.ts", 
    "**/node_modules/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "readme": "README.md",
  "name": "pnwkit 3.0",
  "includeVersion": true,
  "plugin": ["typedoc-plugin-markdown"],
  "outputFileStrategy": "modules",
  "categorizeByGroup": true,
  "validation": {
    "notExported": false
  }
}